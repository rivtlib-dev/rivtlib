{
    "cSpell.enabled": false,
    "cSpell.words": [
        "rivtlib",
        "rivt",
        "rivtzip"
    ],
    "fsdeploy.nodes": [
        {
            "source": "C:/Python313/Lib/site-packages/rivtlib",
            "target": "c:/git/rivtlib/",
            "include": "**/*.*",
            "exclude": "",
            "deleteTargetOnDeploy": true,
        }
    ],
    "fsdeploy.deployOnSave": true,
    "fsdeploy.deployWorkspaceOnSave": false,
    "ShortcutMenuBar.navigateBack": false,
